<h1>Jewelry Shop</h1>
<div class="page col-sm-12">
    <% @items.each do |item| %>
      <div class="col-sm-4">
        <a href="/jewelry/<%= item.id %>">
          <h4><%= item.metal %> <%= item.gemstone %> <%= item.kind %></h4>
          <img src="<%=item.image%>"/>
          <p><%= item.price %></p>
        </a>
      </div>
    <%end%>
</div>